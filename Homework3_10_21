#qeution numebr 1:
i = 1
first = int(input('enter a number: '))

while i <= 9:
    second = int(input('enter a number: '))
    if second < first:
        print('not sorted')
        break
    first = second
    i = i+1

    if i == 10:
        print('sorted')


#quetion number 2:
i = 1
_sum = int(input('enter a number: '))

while i <9:
    x = int(input('enter a number:'))
    if x == _sum:
        break
    _sum = _sum + x
    i = i + 1

print('_sum', _sum)
