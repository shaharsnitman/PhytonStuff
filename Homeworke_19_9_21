# question number 7 & 8

largest = None
smallest = None

while True :
    num = input('Enter a number: ')
    if num == "0" :
        break
    try :
        num = int(num)
    except :
        print('Wrong input')
        continue
    if largest is None :
        largest = num
    elif largest < num :
        largest = num
    if smallest is None :
        smallest = num
    elif num < smallest :
        smallest = num

print('Maximum is', largest)
print('Minimum is', smallest)



# question number 23

def print_factors(x):
   print('The factors of',x,'are:')
   for i in range(1, x + 1):
       if x % i == 0:
           print(i)

num = int(input('Your Number : '))

print_factors(num)


